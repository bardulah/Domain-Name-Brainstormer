# Environment Configuration
# Copy this file to .env and adjust values as needed

# Application environment
NODE_ENV=development

# Logging
LOG_LEVEL=info
LOG_FORMAT=simple

# WHOIS Checker Settings
WHOIS_TIMEOUT=10000
MAX_CONCURRENT=5
MAX_RETRIES=2
RETRY_DELAY=2000

# Cache Settings
CACHE_ENABLED=true
CACHE_TTL=86400000
CACHE_NAMESPACE=domain-brainstormer
CACHE_STORE=./.cache

# Generator Settings
MIN_SCORE=55
MAX_SUGGESTIONS=20

# Default TLDs (comma-separated)
DEFAULT_TLDS=.com,.io,.dev,.co,.net,.app,.ai
