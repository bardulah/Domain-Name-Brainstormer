{
  "name": "domain-brainstormer",
  "version": "3.0.0",
  "description": "Production-ready domain name generation and availability checking",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "domain-brainstorm": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build"
  },
  "keywords": [
    "domain",
    "dns",
    "whois",
    "domain-names",
    "branding",
    "production"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "convict": "^6.2.4",
    "dotenv": "^16.3.1",
    "keyv": "^4.5.4",
    "keyv-file": "^0.2.0",
    "ora": "^5.4.1",
    "whois-json": "^2.0.7",
    "winston": "^3.11.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/convict": "^6.1.6",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.7",
    "@types/yargs": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
